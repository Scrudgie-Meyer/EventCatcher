<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="NewEvent.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:NewEvent"
    Shell.FlyoutBehavior="Disabled">

    <TabBar>
        <Tab>
            <ShellContent Title="" ContentTemplate="{DataTemplate local:MainPage} " />
            <ShellContent Title="" ContentTemplate="{DataTemplate local:EventCreator}" />
            <ShellContent Title="" ContentTemplate="{DataTemplate local:JoinEvent}" />
            <ShellContent Title="" ContentTemplate="{DataTemplate local:Profile}" />
        </Tab>
    </TabBar>
    
</Shell>
